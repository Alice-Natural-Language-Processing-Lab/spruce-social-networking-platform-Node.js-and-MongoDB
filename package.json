{
  "name": "spruce-app",
  "version": "2.0.0",
  "description": "spruce app to control and start spruce",
  "main": "www",
  "scripts": {
    "start": "electron .",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "rm -rf dist; rm -rf .spruce; electron-builder",
    "postinstall": "electron-rebuild -v `node -e \"console.log(require('./package.json').devDependencies['electron'].slice(1));\"`"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dan-divy/spruce.git"
  },
  "build": {
    "productName": "Spruce App",
    "artifactName": "${productName}-${os}-${version}.${ext}",
    "appId": "com.github.dan-divy.Spruce"
  },
  "author": "dan-divy",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dan-divy/spruce/issues"
  },
  "homepage": "https://github.com/dan-divy/spruce#readme",
  "dependencies": {
    "file-system": "^2.2.2",
    "node-fetch": "^2.6.0",
    "nodegit": "^0.26.2",
    "npm": "^6.12.0",
    "ps-node": "^0.1.6"
  },
  "devDependencies": {
    "electron": "^6.0.11",
    "electron-builder": "^21.2.0"
  }
}
